            },
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "ChapterRequestDTO": {
        "required": [
          "book_Id",
          "chapter_Title"
        ],
        "type": "object",
        "properties": {
          "book_Id": {
            "type": "string",
            "format": "uuid"
          },
          "chapter_Title": {
            "minLength": 1,
            "type": "string"
          },
          "lessons": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/LessonRequestDTO"
            },
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "LessonRequestDTO": {
        "required": [
          "chapter_Id",
          "lesson_Title"
        ],
        "type": "object",
        "properties": {
          "lesson_Title": {
            "minLength": 1,
            "type": "string"
          },
          "content": {
            "type": "string",
            "nullable": true
          },
          "chapter_Id": {
            "type": "string",
            "format": "uuid"
          }
        },
        "additionalProperties": false
      },
      "LoginDTO": {
        "type": "object",
        "properties": {
          "account": {
            "type": "string",
            "nullable": true
          },
          "password": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "ManageBookRequestDTO": {
        "type": "object",
        "properties": {
          "bookId": {
            "type": "string",
            "format": "uuid"
          },
          "dateUpdated": {
            "type": "string",
            "format": "date-time"
          }
        },
        "additionalProperties": false
      },
      "ManageChapterDTO": {
        "type": "object",
        "properties": {
          "user_Id": {
            "type": "string",
            "format": "uuid"
          },
          "chapter_Id": {
            "type": "string",
            "format": "uuid"
          },
          "updatedDate": {
            "type": "string",
            "format": "date-time"
          }
        },
        "additionalProperties": false
      },
      "ManageLessonDTO": {
        "type": "object",
        "properties": {
          "user_Id": {
            "type": "string",
            "format": "uuid"
          },
          "lesson_Id": {
            "type": "string",
            "format": "uuid"
          },
          "updatedDate": {
            "type": "string",
            "format": "date-time"
          }
        },
        "additionalProperties": false
      },
      "PlantIdentificationResult": {
        "type": "object",
        "properties": {
          "isSuccess": {
            "type": "boolean"
          },
          "plantName": {
            "type": "string",
            "nullable": true
          },
          "scientificName": {
            "type": "string",
            "nullable": true
          },
          "probability": {
            "type": "number",
            "format": "double"
          },
          "suggestions": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/PlantSuggestion"
            },
            "nullable": true
          },
          "similarImages": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/SimilarImage"
            },
            "nullable": true
          },
          "errorMessage": {
            "type": "string",
            "nullable": true
          },
          "remainingRequests": {
            "type": "integer",
            "format": "int32"
          }
        },
        "additionalProperties": false
      },
      "PlantSuggestion": {
        "type": "object",
        "properties": {
          "plantName": {
            "type": "string",
            "nullable": true
          },
          "scientificName": {
            "type": "string",
            "nullable": true
          },
          "probability": {
            "type": "number",
            "format": "double"
          },
          "commonNames": {
            "type": "array",
            "items": {
              "type": "string"
            },
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "SimilarImage": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "nullable": true
          },
          "url": {
            "type": "string",
            "nullable": true
          },
          "similarity": {
            "type": "number",
            "format": "double"
          }
        },
        "additionalProperties": false
      },
      "UserRequestDTO": {
        "required": [
          "account",
          "fullName",
          "password",
          "role"
        ],
        "type": "object",
        "properties": {
          "account": {
            "maxLength": 50,
            "minLength": 0,
            "type": "string"
          },
          "password": {
            "maxLength": 100,
            "minLength": 6,
            "type": "string"
          },
          "role": {
            "minLength": 1,
            "pattern": "^(Teacher|Student|Admin)$",
            "type": "string"
          },
          "fullName": {
            "maxLength": 100,
            "minLength": 0,
            "type": "string"
          }
        },
        "additionalProperties": false
      }
    },
    "securitySchemes": {
      "Bearer": {
        "type": "apiKey",
        "description": "JWT Authorization header using the Bearer scheme. Enter 'Bearer' [space] and then your token in the text input below.",
        "name": "Authorization",
        "in": "header"
      }
    }
  },
  "security": [
    {
      "Bearer": [ ]
    }
  ]
