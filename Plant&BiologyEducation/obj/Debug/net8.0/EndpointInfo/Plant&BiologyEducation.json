  "components": {
    "schemas": {
      "BookRequestDTO": {
        "required": [
          "book_Title"
        ],
        "type": "object",
        "properties": {
          "book_Title": {
            "minLength": 1,
            "type": "string"
          },
          "cover_img": {
            "type": "string",
            "nullable": true
          },
          "chapters": {
            "type": "array",
