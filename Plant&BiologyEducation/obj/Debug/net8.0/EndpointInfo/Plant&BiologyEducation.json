  "components": {
    "schemas": {
      "LoginDTO": {
        "type": "object",
        "properties": {
          "account": {
            "type": "string",
            "nullable": true
          },
          "password": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "QuestionRequestDTO": {
        "required": [
          "answer",
          "content",
          "testId"
        ],
        "type": "object",
        "properties": {
          "content": {
            "maxLength": 1000,
            "minLength": 0,
            "type": "string"
          },
          "answer": {
            "maxLength": 500,
            "minLength": 0,
            "type": "string"
          },
          "testId": {
            "minLength": 1,
            "type": "string"
          }
        },
        "additionalProperties": false
      },
      "SubmitTestDTO": {
        "type": "object",
        "properties": {
          "userId": {
            "type": "string",
            "format": "uuid"
          },
          "testId": {
            "type": "string",
            "nullable": true
          },
          "result": {
            "type": "number",
            "format": "double"
          }
        },
        "additionalProperties": false
      },
      "TestRequestDTO": {
        "required": [
          "creatorId"
        ],
        "type": "object",
        "properties": {
          "creatorId": {
            "type": "string",
            "format": "uuid"
          },
          "dateCreated": {
            "type": "string",
            "format": "date-time"
          }
        },
        "additionalProperties": false
      },
      "UserRequestDTO": {
        "required": [
          "account",
          "fullName",
          "password",
          "role"
        ],
        "type": "object",
        "properties": {
          "account": {
            "maxLength": 50,
            "minLength": 0,
            "type": "string"
          },
          "password": {
            "maxLength": 100,
            "minLength": 6,
            "type": "string"
          },
          "role": {
            "minLength": 1,
            "pattern": "^(Teacher|Student)$",
            "type": "string"
          },
          "fullName": {
            "maxLength": 100,
            "minLength": 0,
            "type": "string"
          }
        },
        "additionalProperties": false
      }
    },
    "securitySchemes": {
      "Bearer": {
        "type": "apiKey",
        "description": "JWT Authorization header using the Bearer scheme. Enter 'Bearer' [space] and then your token in the text input below.",
        "name": "Authorization",
        "in": "header"
      }
    }
