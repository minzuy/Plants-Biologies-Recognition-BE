          },
          "dateCreated": {
            "type": "string",
            "format": "date-time"
          }
        },
        "additionalProperties": false
      },
      "UserRequestDTO": {
        "required": [
          "account",
          "fullName",
          "password",
          "role"
        ],
        "type": "object",
        "properties": {
          "account": {
            "maxLength": 50,
            "minLength": 0,
            "type": "string"
          },
          "password": {
            "maxLength": 100,
            "minLength": 6,
            "type": "string"
          },
          "role": {
            "minLength": 1,
            "pattern": "^(Admin|Teacher|Student)$",
            "type": "string"
          },
          "fullName": {
            "maxLength": 100,
            "minLength": 0,
            "type": "string"
